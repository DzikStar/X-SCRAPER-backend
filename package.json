{
  "name": "x-scraper",
  "version": "1.0.0-development",
  "description": "Engine for scraping x.com platform and multi-platform flags.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "tsx --env-file-if-exists=.env src/index.js",
    "dev": "tsx --watch --env-file .env src/index.ts",
    "check-format": "prettier --check ./src",
    "format": "prettier --write ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DzikStar/X-SCRAPER-backend.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/node": "^22.12.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^15.15.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.29.1"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  }
}
